{
  "name": "adonis-captcha",
  "version": "0.0.3",
  "description": "Captcha provider for adonisjs framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win"
  },
  "keywords": [
    "adonis",
    "adonis-framework",
    "recaptcha",
    "captcha"
  ],
  "author": "yariksav",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.8",
    "@adonisjs/sink": "^1.0.16",
    "coveralls": "^3.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^5.0.1",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "node-cache-promise": "^1.0.0",
    "nyc": "^11.7.1",
    "semver": "^5.5.0",
    "standard": "^11.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "bin"
    ]
  },
  "standard": {
    "globals": [
      "use"
    ]
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.0",
    "recaptcha2": "^1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yariksav/adonis-captcha.git"
  },
  "bugs": {
    "url": "https://github.com/yariksav/adonis-captcha/issues"
  },
  "homepage": "https://github.com/yariksav/adonis-captcha#readme"
}
